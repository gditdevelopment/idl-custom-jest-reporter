{
  "name": "@jest-performance-reporter/example",
  "private": "true",
  "version": "2.1.2",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "pretest:integration": "yarn test src/*.spec.ts",
    "test:integration": "jest __tests__/jest-integration.spec.ts --reporters='default'"
  },
  "dependencies": {
    "@jest-performance-reporter/core": "^2.1.2"
  },
  "devDependencies": {
    "@swc/core": "1.2.114",
    "@swc/jest": "0.2.11",
    "@types/jest": "27.0.3",
    "csv-parser": "3.0.0",
    "jest": "27.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "reporters": [
      "default",
      [
        "@jest-performance-reporter/core",
        {
          "errorAfterMs": 1000,
          "warnAfterMs": 500,
          "logLevel": "warn",
          "maxItems": 5,
          "jsonReportPath": "performance-report.json",
          "csvReportPath": "performance-report.csv"
        }
      ]
    ]
  }
}
